{
  "non-interactive": false,
  "dry-run": false,
  "verbose": false,
  "debug": false,
  "force": false,
  "pkgFiles": ["package.json", "package-lock.json"],
  "increment": "patch",
  "preReleaseId": null,
  "buildCommand": "npm run build && node ./workflow/generatechangelog.js",
  "safeBump": true,
  "beforeChangelogCommand": false,
  "changelogCommand":
    "git log --branches --not master --remotes --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%Creset' --abbrev-commit",
  "requireCleanWorkingDir": true,
  "requireUpstream": true,
  "src": {
    "commit": true,
    "commitMessage": "Release %s",
    "commitArgs": "--no-verify",
    "tag": true,
    "tagName": "%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "pushArgs": "",
    "pushRepo": null,
    "beforeStartCommand": false,
    "afterReleaseCommand": false,
    "addUntrackedFiles": true
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": false,
    "releaseName": "Release %s",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": null,
    "host": null,
    "timeout": 0
  },
  "dist": {
    "repo": false,
    "stageDir": ".stage",
    "baseDir": "dist",
    "files": ["**/*"],
    "pkgFiles": null,
    "commit": true,
    "commitMessage": "Release %s",
    "commitArgs": "",
    "tag": true,
    "tagName": "%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "pushArgs": "",
    "beforeStageCommand": false,
    "afterReleaseCommand": false,
    "addUntrackedFiles": false,
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  },
  "prompt": {
    "src": {
      "status": false,
      "commit": true,
      "tag": true,
      "push": true,
      "release": true,
      "publish": true
    },
    "dist": {
      "status": false,
      "commit": true,
      "tag": false,
      "push": true,
      "release": false,
      "publish": false
    }
  }
}
